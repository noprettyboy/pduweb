/*src/css/ddsmoothmenu.css*/
@charset "utf-8";
/* CSS Document */

img a{
  border:none;
}

.ddsmoothmenu{
  font:normal 12px Arial;
  background: none; /*background of menu bar (default state)*/
  width: 100%;
}

.ddsmoothmenu ul{
z-index:100;
margin: 0;
padding: 0;
list-style-type: none;
}

/*Top level list items*/
.ddsmoothmenu ul li{
position: relative;
display: inline;
float: left;
}

/*Top level menu link items style*/
.ddsmoothmenu ul li a {
  color: #fff;
  display: block;
  padding: 7px 10px;
  text-decoration: none;
  height: 14px;

  width: 184px;
}

.ddsmoothmenu ul li a.selected {
  color: #000;
}

.ddsmoothmenu ul li a .menu-arr-down,
.ddsmoothmenu ul li a .menu-arr-right{
  /*
  background-position: 0 4px;
  */
  display: block;
  float: right;
  height: 16px;
  /*margin-left: 6px;*/
  margin-left: 6px;
  width: 12px;
  /*width: 12px;*/
}

.ddsmoothmenu ul li a .menu-arr-down {
  float: left;
  margin: 1px 6px 0 0;
}

.ddsmoothmenu ul li.selected{
  color: #000;
  background: repeat-x;
  background-image: url('img/s.png');
  background-position: left -1533px;
}

.ddsmoothmenu ul li:hover {
  color: #000;
  background: repeat-x;
  background-image: url('img/s.png');
  background-position: left -1217px;
  /*background-image: url(../images/MenuBgHover.gif);    */
}

	
/*1st sub level menu*/
.ddsmoothmenu ul li ul{
  box-shadow: 2px 2px 2px #999;
  position: absolute;
  left: 0;
  display: none;
  visibility: hidden;
  color:#000;
}

/*Sub level menu list items (undo style from Top level List Items)*/
.ddsmoothmenu ul li ul li
{
  background:#f9f9f9;
  display: list-item;
  float: none;
  color:#000;
}

/*All subsequent sub menu levels vertical offset after 1st level sub menu */
.ddsmoothmenu ul li ul li ul
{
color:#000;
top: 0;
}

.icons{border:none; padding-right:6px;}

/* Sub level menu links style */
.ddsmoothmenu ul li ul li a
{
  border-top-width: 0;
  border-bottom: 1px solid gray;
  border-right: 1px solid gray;
  border-left: 1px solid gray;

  border-width: 0 1px 1px;
  color: #000000;
  font: 11px Arial,Helvetica,sans-serif;
  margin: 0;
  padding: 5px 5px 5px 28px;

  width: 170px;
}

#forcehttps a{
  padding: 5px;
  width: 191px;
}


/* Holly Hack for IE \*/
* html .ddsmoothmenu{height: 1%;} /*Holly Hack for IE7 and below*/


/* ######### CSS classes applied to down and right arrow images  ######### */

.downarrowclass{
  position: absolute;
  top: 12px;
  right: 7px;
}

.rightarrowclass{
  position: absolute;
  top: 6px;
  right: 5px;
}

/* ######### CSS for shadow added to sub menus  ######### */

.ddshadow{ /*shadow for NON CSS3 capable browsers*/

}

.toplevelshadow{ /*shadow opacity for NON CSS3 capable browsers. Doesn't work in IE*/
  opacity: 0;
}

.ui-dialog-title div {
  height: 22px;
  line-height: 22px;
  padding-left: 24px;
}
/*src/css/chrome.css*/
.chromestyle:after{
  content:".";
  display:block;height:0;clear:both;visibility:hidden
}
.chromestyle ul{width:72px;height:27px;display:block;padding:3px 0 0 0;margin:0}
.chromestyle ul li{display:inline-block;border:0; font-size:12px}
.chromestyle ul li a {
  color: #FFF;
  display: block;
  padding: 0 7px;
  text-decoration: none;
  width: 46px;
}
.chromestyle ul li a:hover,.chromestyle ul li a.selected{display:block;color:#fff}
.dropmenudiv{
  position:absolute;top:0;right:0;border:1px solid #b5e2fe;
  border-bottom-width:0;font:normal 11px Arial;line-height:18px;
  z-index:100;background-color:white;width:200px;visibility:hidden
}

.dropmenudiv a{
  border-bottom: 1px solid #B5E2FE;
  color: black;
  display: block;
  height: 14px;
  line-height: 14px;
  padding: 5px 5px 5px 24px;
  text-decoration: none;
  text-indent: 3px;
  width: 100%;
}

.dropmenudiv li:hover{
  /*background: url(../images/MenuBgHover.gif) center repeat-x;*/
  background-repeat: repeat-x;
  background-position: center;
  background-image: url('img/s.png');
  background-position: left -1217px;
}

.chromestyle .menu-arr-down{
  /*
  background-position: 0 4px;
  */
  display: block;
  float: right;
  height: 16px;
  margin: 4px 0 2px 5px;
  width: 12px;
}
/*src/css/modules.css*/
/*
Global style
*/
body{
  font-size:12px;
}

html, body{
  overflow:hidden;
}

.grad-login{
  background:#E2EFFE;
}

.disp{
  display:none;
}

.opacityElement {
  filter:alpha(opacity=10);  // IE
  -moz-opacity:0.1;          // Firefox
  -khtml-opacity: 0.1;
  opacity: 0.6;
}

.selected{
  display:none;
}

img a{
  border:none;
}

.hidden{
  display: none;
}


/*
* generic style
*/
.hide,
.form-cols2 label.hide,
.formval label.hide
{
  display:none;
}

.clear{
  clear: both;
}

.gridwrap{
  height:100%;
  width:100%;
}

.btn{
  background: none repeat scroll 0 0 #EFFFFD;
  border: 1px solid #A4D2FF !important;
  border-radius: 0.4em;
  color: #2E6E9E !important;
  cursor: pointer;
  display: inline-block;
  font-weight: 700;
  margin: 0 0.2em;
  padding: 0.2em 1em;
  text-decoration: none;
}

/*like a*/
.btn.disable,
.btn.disable:hover,
/*button on the dialog*/
.btn.locked,
.btn.locked:hover,
.ui-widget-content input.locked,
.ui-widget-content button.locked
{
  /*background: url("../images/locked.png") no-repeat scroll 3px 1px #DEDEDE;*/
  background-repeat: no-repeat;
  background-attachment: scroll;
  background-position: 3px 1px;
  background-color: #DEDEDE;
  background-image: url('img/s.png');
  background-position: left -7276px;
  padding-left: 18px;
}

/*locked a layer*/
.locklayer{
  filter:alpha(opacity=15);
  opacity: 0.15;
  background-color: #000;
  display: none;
  height: 100%;
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  z-index: 1000;
}

#roleHolder{
  position: relative;
}

.btn:hover{
  background: #dfeffc;
}


.btn.full{
  display: block;
  margin: 0 0 10px;
  padding: 1em;
  text-align: center;
  width: auto;
}

.bottom{
  position: absolute;
  bottom: -32px;
  visibility: hidden;
}

.bottom input{

}

/*the bottom container only displayed in the dialog box*/
.ui-dialog .bottom{
  visibility: visible;
}

/*Fixed tab*/
.ui-dialog .ui-tabs .ui-tabs-nav li.ui-tabs-selected{
  top: 2px;
}

